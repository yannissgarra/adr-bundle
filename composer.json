{
    "name": "webmunkeez/adr-bundle",
    "type": "symfony-bundle",
    "description": "Action-Domain-Responder pattern made for Symfony.",
    "keywords": ["Symfony", "controllers", "action", "autowiring", "responder", "templates"],
    "homepage": "https://github.com/yannissgarra/adr-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannis Sgarra",
            "email": "hello@yannissgarra.com",
            "homepage": "https://yannissgarra.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "doctrine/annotations": "^1.0",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/http-foundation": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/serializer": "^5.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpdocumentor/reflection-docblock": "^5.0",
        "phpunit/phpunit": "^9.0",
        "symfony/browser-kit": "^5.0",
        "symfony/css-selector": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Webmunkeez\\AdrBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmunkeez\\AdrBundle\\Test\\": "tests/"
        }
    }
}
